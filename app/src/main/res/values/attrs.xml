<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="TextInputView">
        <attr name="labelText" format="string" />
        <attr name="hintText" format="string" />
        <attr name="inputTypeAttr" format="enum">
            <enum name="text" value="0" />
            <enum name="password" value="1" />
            <enum name="email" value="2" />
            <enum name="phone" value="3" />
            <enum name="number" value="4" />
            <enum name="multiline" value="5" />
        </attr>
        <attr name="drawableEnd" format="reference"/>
    </declare-styleable>

</resources>